<div class="panel panel-danger">
    <div class="panel-heading">
      <h3 class="panel-title">Corregir errores</h3>
    </div>
    <div class="panel-body">
      <p class="text-danger">
      <% for (var i in errors) { %>
        <span><%= errors[i].message %></span><br/>
      <% } %>
      </p>
    </div>
</div>
